{
  // Cloudflare Wrangler configuration for RouteCoinMe
  // This file configures deployment settings for Cloudflare Pages
  
  "name": "routecoinme",
  "compatibility_date": "2025-11-29",
  "compatibility_flags": ["nodejs_compat"],
  
